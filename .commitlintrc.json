{
    "extends": [
        "@commitlint/config-conventional"
    ],
    "rules": {
        "type-enum": [
            2,
            "always",
            [
                "feat",
                "fix",
                "chore",
                "test",
                "doc",
                "refactor",
                "perf",
                "style",
                "bot"
            ]
        ]
    }
}